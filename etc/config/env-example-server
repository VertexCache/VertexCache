#
# VertexCache Server - Default Property File
#
# Default settings enable TLS/SSL and the use of Public/Private Keys. Please do not use this in a production
# environment; it is solely intended to help you get up and running as quickly as possible. The client console is
# configured the same way, allowing both to communicate with each other out of the box.
#
# Full Documentation available at the VertexCache Wiki https://github.com/jasonlam604/VertexCache/wiki
#

# Verbose Output, default false
enable_verbose=true

# Server Port, default 50505
server_port=50505

# Cache Eviction Policy
#
#   Available options:
#
#    NONE - No Eviction applied, unbound your limit will be the available memory
#    LRU - Least Recently Used
#    MRU - Most Recently Used
#    FIFO - First In and First Out
#    LFU - Least Frequently Used
#    RANDOM - Random
#    ARC - Adaptive Replacement Cache
#    2Queue - Two Queues
#    Clock - Clock (or Clock-Pro)
#    TinyLFU - Tiny Least Frequently Used
#
cache_eviction=LRU

# this is required when using eviction policy other than NONE
cache_size=100000

########################################################################################################################
# Encrypt Message Layer public/private keys, default false
#
#   *** WARNING ***
#
#   This are test keys, DO NOT USE IN PRODUCTION
#
#   When enable_encrypt_message=true, then you can have the option to ONLY use RSA or AES
#   RSA is public/private key and AES is AES symmetric encryption (shared key)
#
#   If both are set an error will happen, if neither are set an error will happen.
#
########################################################################################################################
enable_encrypt_message=true


